{
  "name": "@pickolor/vue",
  "type": "module",
  "version": "0.1.0",
  "description": "Vue bindings for Pickolor core APIs",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsup --config tsup.config.ts",
    "lint": "eslint src --ext .ts,.tsx,.vue",
    "test": "vitest run"
  },
  "peerDependencies": {
    "@pickolor/core": "workspace:*",
    "vue": ">=3.3.0"
  },
  "devDependencies": {
    "@pickolor/core": "workspace:*",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15",
    "vue": "^3.5.12"
  }
}
