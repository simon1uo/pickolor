{
  "name": "@pickolor/react",
  "type": "module",
  "version": "0.1.0",
  "description": "React bindings for Pickolor core APIs",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsup --config tsup.config.ts",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "vitest run"
  },
  "peerDependencies": {
    "@pickolor/core": "workspace:*",
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
  "devDependencies": {
    "@pickolor/core": "workspace:*",
    "@types/node": "^24.10.1",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  }
}
